	},
	{
		"path": "pages/pickCar/index",
		"style": {
			"navigationBarTitleText": "接车单"
		}
	},
	{
		"path": "pages/scan/index",
		"style": {
			"navigationBarTitleText": "扫描详情"
		}
	},
	{
		"path": "pages/carInfo/index",
		"style": {
			"navigationBarTitleText": "录入车辆信息"
		}
	},
	{
		"path": "pages/preCheckProject/index",
		"style": {
			"navigationBarTitleText": "选检测项目"
		}
	},{
		"path": "pages/preCheckProject/success"
	}, {
		"path": "pages/checkModels/index"
	}, {
		"path": "pages/checkModels/info"
	}],
